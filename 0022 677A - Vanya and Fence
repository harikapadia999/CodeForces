#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define fast ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
int main()
{
	fast
    int n,h,in=0,out=0;cin>>n>>h;
    for(int i=0;i<n;i++){
        cin>>in;
        if(in>h)out+=2;
        else out+=1;
    }
    cout<<out<<endl;
    return 0;
}






