#include<bits/stdc++.h>
#define ll long long
using namespace std;
#define fast ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
void solve(){
    int a,b;cin>>a>>b;
    if(a%b==0)cout<<0<<endl;
    else cout<<b-(a%b)<<endl;
}

int main()
{
	fast
	ll t;cin >> t;
	while (t--)
    solve();  
    return 0;
}






