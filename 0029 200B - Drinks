#include<bits/stdc++.h>
#define ll long long
using namespace std;
#define fast ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
int main()
{
	fast
    int n,c;
    double ans=0;cin >> n;
    c=n;
    while (n>0){
        int t;cin >> t;
        ans+=t;
        n--;
    }
    cout<<ans/c<<endl;
    return 0;
}






