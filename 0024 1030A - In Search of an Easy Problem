#include<bits/stdc++.h>
#define ll long long
using namespace std;
#define fast ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
int main()
{
	fast
    ll n,h=0;cin >> n;
    while(n--){
        ll a;cin >> a;
        if(a == 1)h++;
    }
    
    if(h==0)cout<<"EASY"<<endl;
    else cout <<"HARD"<<endl;
    return 0;
}
