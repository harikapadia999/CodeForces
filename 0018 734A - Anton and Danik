#include<bits/stdc++.h>
using namespace std;
#define fast ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
int solve(){
	int n,a=0,d=0;cin>>n;
	string s;cin>>s;
	for(int i=0;i<n;i++){
		if(s[i]=='A')a++;
		else d++;
	}
	if(a>d)cout<<"Anton"<<endl;
	else if(a<d)cout<<"Danik"<<endl;
	else cout<<"Friendship"<<endl;
}
int main()
{
	fast
	solve();
    return  0;
}

/*

for(int i=0; i<n; i++): This is a loop that iterates n times, with the loop variable i ranging from 0 to n-1.

if(s[i]=='A') a++; else d++;: This line checks whether the i-th character of the string s is 'A'. If it is, it increments the variable a, 
otherwise it increments the variable d.

if(a>d) cout<<"Anton"<<endl; else if(a<d) cout<<"Danik"<<endl; else cout<<"Friendship"<<endl;
This block of code checks whether the number of occurrences of 'A' in the string s is greater than, less than, or equal to the number of occurrences of 'D'. 
Depending on the result, it prints either "Anton", "Danik", or "Friendship" to stdout.


*/
